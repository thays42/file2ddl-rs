{
  "name": "Oracle",
  "type_mappings": {
    "Boolean": "CHAR(1)",
    "SmallInt": "NUMBER(5)",
    "Integer": "NUMBER(10)",
    "BigInt": "NUMBER(19)",
    "DoublePrecision": "BINARY_DOUBLE",
    "Date": "DATE",
    "Time": "TIMESTAMP",
    "DateTime": "TIMESTAMP",
    "Varchar": "VARCHAR2({})",
